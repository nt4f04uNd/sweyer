{
  "problemMatcher": [
    {
      "owner": "flutter-analyze",
      "pattern": [
        {
          "regexp": "^\\s*(error|warning) • (.*?) • (.*):([0-9]+):([0-9]+) • (.*)$",
          "severity": 1,
          "message": 2,
          "file": 3,
          "line": 4,
          "column": 5,
          "code": 6
        }
      ]
    },
    {
      "owner": "flutter-analyze-info",
      "severity": "notice",
      "pattern": [
        {
          "regexp": "^\\s*info • (.*?) • (.*):([0-9]+):([0-9]+) • (.*)$",
          "message": 1,
          "file": 2,
          "line": 3,
          "column": 4,
          "code": 5
        }
      ]
    }
  ]
}
