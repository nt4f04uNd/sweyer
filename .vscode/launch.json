   "version": "0.2.0",
   "configurations": [
      {
         "name": "dev",
         "type": "dart",
         "request": "launch",
         "program": "lib/main.dart",
      },
      {
         "name": "profile",
         "type": "dart",
         "request": "launch",
         "program": "lib/main.dart",
         "flutterMode": "profile"
      },
      {
         "name": "release",
         "type": "dart",
         "request": "launch",
         "program": "lib/main.dart",
         "flutterMode": "release"
      },
      {
         "name": "Golden",
         "request": "launch",
         "type": "dart",
         "codeLens": {
            "for": [
               "run-test",
               "run-test-file",
               "debug-test",
               "debug-test-file",
            ],
            "title": "${debugType} golden",
         },
         "args": [
            "--update-goldens",
         ]
      },
   ]
}